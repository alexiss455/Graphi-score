<div class="account-login">
    <div class="profile-size">
        <div class="profile-circle account-login-navs">
            <% if(profile){ %>
                <img src="<%= profile%>" alt="welcome_img" class="profile">
            <% }else{ %>
                <img src="/img/My_project1.png" alt="welcome_img" class="profile">
            <% } %>
            
        </div>
        </div>
        <div class="profile-name-nav">
            <%= storeName %>
            <div class="profile-nav-settings">
                <ul class="profile-link">
                    <li><a href="/profile"><i class='bx bxs-user-account bxp'></i>Profile</a></li>
                    <li><a href="/account-settings"><i class='bx bx-cog bxp'></i>Account Settings</a></li>
                    <li><a href="/logout"><i class='bx bx-log-out bxp'></i>Sign Out</a></li>
                </ul>
            </div>
        </div>
    </div>









